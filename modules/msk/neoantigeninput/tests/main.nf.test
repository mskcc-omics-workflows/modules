nextflow_process {

    name "Test Process NEOANTIGENINPUT"
    script "../main.nf"
    process "NEOANTIGENINPUT"

    tag "modules"
    tag "modules_msk"
    tag "neoantigeninput"

    test("neoantigen") {

        when {
            params {
                folderPath = 'tst'
            }

            process {
                """
                
                file(params.folderPath).mkdirs()
                originalFile = file(params.test_data_mskcc['neoantigen']['test4_mutass_0_json'], checkIfExists: true)
                copy_1 = originalFile.copyTo(file("0.json"))
                copy_2 = originalFile.copyTo(file("1.json"))
                copy_1.moveTo(file(params.folderPath + "/0.json"))
                copy_2.moveTo(file(params.folderPath + "/1.json"))

                input[0] = [
                    [ id:'test', single_end:false ], // meta map
                    file(params.test_data_mskcc['neoantigen']['temp_test_maf'], checkIfExists: true)
                    ]

                input[1] = [
                    file(params.test_data_mskcc['neoantigen']['test4_summ_json'], checkIfExists: true),
                    file(params.test_data_mskcc['neoantigen']['test4_muts_json'], checkIfExists: true),
                    file(params.folderPath, checkIfExists: true)
                    ]
                
                input[2] = [
                    [ id:'test', single_end:false ], // meta map
                    file(params.test_data_mskcc['neoantigen']['MUTnetMHCpan'], checkIfExists: true),
                    file(params.test_data_mskcc['neoantigen']['WTnetMHCpan'], checkIfExists: true)
                    ]

                input[3] = [file(params.test_data_mskcc['neoantigen']['winners_hla_txt'], checkIfExists: true)]
        
                """
            }
        }

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(process.out).match() }
            )
        }

    }

}

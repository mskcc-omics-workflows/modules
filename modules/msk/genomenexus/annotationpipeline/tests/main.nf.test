
// nf-core modules test genomenexus/annotationpipeline
nextflow_process {

    name "Test Process GENOMENEXUS_ANNOTATIONPIPELINE"
    script "../main.nf"
    process "GENOMENEXUS_ANNOTATIONPIPELINE"

    tag "modules"
    tag "modules_nfcore"
    tag "modules_msk"
    tag "genomenexus"
    tag "genomenexus/annotationpipeline"

    
    test("test normal ") {


        when {
            process {
                """
                
                input[0] = [
                    [ id:'test'], // meta map
                    file("/home/naidur/test_data/mod.maf", checkIfExists: true)
                    ]
                """
            }
        }

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(process.out).match() }
                
            )
        }

    }

    
    test("test normal - stub") {

        options "-stub"

        when {
            process {
                """
                
                
                input[0] = [
                    [ id:'test'], // meta map
                    file("/home/naidur/test_data/mod.maf", checkIfExists: true)
                    ]
                """
            }
        }

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(process.out).match() }
                
            )
        }

    }

}

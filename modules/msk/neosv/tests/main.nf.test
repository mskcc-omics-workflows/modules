nextflow_process {

    name "Test Process NEOSV"
    script "../main.nf"
    process "NEOSV"

    tag "modules"
    tag "modules_nfcore"
    tag "neosv"
    tag "modules_msk"

    test("netmhcpan - MUT - xls,output,fa") {

        when {
            process {
                """
                input[0] = [
                    [ id:'test', single_end:false ], // meta map
                    file(file(params.test_data_mskcc['neoantigen']['svbedpe']), checkIfExists: true),
                    HLA-A24:02,HLA-A24:02,HLA-B39:01,HLA-B39:01,HLA-C07:01,HLA-C06:02,
                    ]
                """
            }
        }

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(
                    process.out.versions,
                    process.out.mutOut,
                    process.out.wtOut,
                    ).match()
                }
            )
        }

    }


    test("netmhcpan - xls,output,fa - stub") {

        options "-stub"

        when {
            process {
                """
                input[0] = [
                    [ id:'test', single_end:false ], // meta map
                    file('MUT_sequence_fa'),
                    "HLA",
                    "MUT"
                    ]
                """
            }
        }

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(
                    process.out.versions,
                    process.out.mutOut,
                    process.out.wtOut,
                    ).match()
                }
            )
        }

    }

}


// nf-core modules test genomenexus/annotationpipeline
nextflow_process {

    name "Test Process ONCOKB_MAFANNOTATOR"
    script "../main.nf"
    process "ONCOKB_MAFANNOTATOR"

    tag "modules"
    tag "modules_nfcore"
    tag "modules_msk"
    tag "oncokb"
    tag "oncokb/mafannotator"


    test("test") {


        when {
            process {
                """

                input[0] = [
                    [ id:'test',case_id:'sample1',control_id:'sample2'], // meta map
                    file(params.test_data_mskcc['genome_nexus']['sample2_sample1_annotated_maf'], checkIfExists: true)
                    ]
                """
            }
        }

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(process.out).match() }

            )
        }

    }


    test("test normal - stub") {

        options "-stub"

        when {
            process {
                """


                input[0] = [
                    [ id:'test',case_id:'sample1',control_id:'sample2'], // meta map
                    file(params.test_data_mskcc['genome_nexus']['sample2_sample1_annotated_maf'], checkIfExists: true)
                    ]
                """
            }
        }

        then {
            assertAll(
                { assert process.success }

            )
        }

    }

}
